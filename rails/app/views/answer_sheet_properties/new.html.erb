<h1>New answer_sheet_property</h1>
<p style="color: green"><%= flash[:notice] %></p>

<%= form_for(@answer_sheet_property) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :answer_sheet_id %><br />
    <%= f.text_field :answer_sheet_id %>
  </p>
  <p>
    <%= f.label :ocr_name %><br />
    <%= f.text_field :ocr_name %>
  </p>
  <p>
    <%= f.label :ocr_value %><br />
    <%= f.text_field :ocr_value %>
  </p>
  <p>
    <%= f.label :ocr_image %><br />
    <%= f.text_field :ocr_image %>
  </p>
  <p>
    <%= f.label :need_check %><br />
    <%= f.check_box :need_check %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', answer_sheet_properties_path %>
